{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "import-name": false,
    "max-line-length": [true, 120],
    "no-var-self": false,
    "ordered-imports": true,
    "strict-boolean-expressions": [true, "allow-null-union", "allow-undefined-union", "allow-string", "allow-number"],
    "variable-name": false
  },
  "rulesDirectory": []
}
